{
    "name": "crawly/crawly",
    "type": "project",
    "description": "The test crawly.",
    "keywords": ["test", "crawly"],
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "8.0.2"
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
